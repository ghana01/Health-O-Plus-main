# ===========================================
# Health-O-Plus Frontend Environment Variables
# ===========================================
# Copy this file to .env and fill in your values
# For Vite projects, all env variables must start with VITE_

# Backend API URL
# For local development: http://localhost:8000/api/v1
# For production: https://your-backend-domain.onrender.com/api/v1
VITE_API_BASE_URL=http://localhost:8000/api/v1

# AI Consult API URL (different path)
# For local development: http://localhost:8000/api/ai-consult
# For production: https://your-backend-domain.onrender.com/api/ai-consult
VITE_AI_CONSULT_URL=http://localhost:8000/api/ai-consult

# WebSocket URL for Video Calls (Optional - auto-detected from API URL)
# For local development: ws://localhost:8000
# For production: wss://your-backend-domain.onrender.com
# VITE_WS_URL=ws://localhost:8000

# Note: Cloudinary is NOT required
# Images are stored as Base64 directly in the database
